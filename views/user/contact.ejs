<%- include('../layouts/user-header') %>

    <section class="contact">

        <h1 class="title"> get in touch </h1>

        <div class="row">

            <form name="submit-to-google-sheet" id="contact-form" action="">
                <div id="msg-report" class="sent-message text-success">Your message has been sent. Thank you!</div>
                <input name="name" type="text" placeholder="your name" class="box" required>
                <input name="number" type="number" placeholder="your number" class="box" required>
                <input name="email" type="email" placeholder="your email" class="box" required>
                <textarea name="message" placeholder="your message" id="" cols="30" rows="10" required></textarea>
                <input type="submit" value="send message" class="btn">
            </form>

            <iframe class="map"
                src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3508.672786519138!2d77.29400317408509!3d28.429128693290977!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x390cdde6132904b1%3A0x3fc29751b7f69281!2sCakes%20&#39;n%20Bakes%20-%20Park%20Plaza!5e0!3m2!1sen!2sin!4v1719756724069!5m2!1sen!2sin"
                allowfullscreen="" loading="lazy"></iframe>
        </div>

    </section>
